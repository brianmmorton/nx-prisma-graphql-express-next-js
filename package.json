{
  "name": "boilerplate",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "generate:component": "nx g @nrwl/next:component my-new-component --project=my-new-app",
    "generate:page": "nx g @nrwl/next:page my-new-page --project=my-new-app",
    "api:dev": "nx serve api",
    "web:dev": "npm run web:generate-types & nx serve web",
    "web:generate-types": "graphql-codegen --config ./apps/web/codegen.yml --watch",
    "api:prisma:generate": "npx prisma generate --schema=./apps/api/prisma/schema.prisma",
    "api:prisma:seed": "npx prisma db seed",
    "api:prisma:migrate": "prisma migrate dev --schema=./apps/api/prisma/schema.prisma"
  },
  "private": true,
  "prisma": {
    "seed": "ts-node ./apps/api/prisma/seed.ts"
  },
  "dependencies": {
    "@apollo/client": "^3.7.9",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@fontsource/roboto": "^4.5.8",
    "@mui/icons-material": "^5.11.9",
    "@mui/material": "^5.11.10",
    "@nrwl/next": "15.7.2",
    "@prisma/client": "^4.10.1",
    "antd": "^5.2.2",
    "apollo-server": "^3.11.1",
    "apollo-server-express": "^3.11.1",
    "axios": "^1.0.0",
    "class-validator": "^0.14.0",
    "express": "^4.18.1",
    "graphql": "^15.3.0",
    "graphql-redis-subscriptions": "^2.6.0",
    "graphql-scalars": "^1.20.1",
    "graphql-tag": "^2.12.6",
    "ioredis": "^5.3.1",
    "next": "13.1.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "reflect-metadata": "^0.1.13",
    "tslib": "^2.3.0",
    "type-graphql": "^1.1.1"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.14.5",
    "@graphql-codegen/cli": "^2.12.0",
    "@graphql-codegen/fragment-matcher": "^3.3.1",
    "@graphql-codegen/typescript": "^2.7.3",
    "@graphql-codegen/typescript-operations": "^2.5.3",
    "@graphql-codegen/typescript-react-apollo": "^3.3.3",
    "@nrwl/cypress": "15.7.2",
    "@nrwl/eslint-plugin-nx": "15.7.2",
    "@nrwl/express": "^15.7.2",
    "@nrwl/jest": "15.7.2",
    "@nrwl/js": "15.7.2",
    "@nrwl/linter": "15.7.2",
    "@nrwl/node": "15.7.2",
    "@nrwl/nx-cloud": "latest",
    "@nrwl/react": "15.7.2",
    "@nrwl/webpack": "15.7.2",
    "@nrwl/workspace": "15.7.2",
    "@testing-library/react": "13.4.0",
    "@types/express": "4.17.13",
    "@types/jest": "28.1.1",
    "@types/node": "18.11.9",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "babel-jest": "28.1.1",
    "cypress": "^12.2.0",
    "eslint": "~8.15.0",

    "jest-environment-jsdom": "28.1.1",
    "nx": "15.7.2",
    "prettier": "^2.6.2",
    "prisma": "^4.10.1",
    "react-test-renderer": "18.2.0",
    "sass": "1.55.0",
    "ts-jest": "28.0.5",
    "ts-node": "10.9.1",
    "typescript": "~4.8.2"
  }
}
